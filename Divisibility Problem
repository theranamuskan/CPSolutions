#include <bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    int t;
    int count = 0;
    cin >> t;
    for (int i = 0; i < t; i++)
    {
        int a, b,rem;
        cin >> a >> b;
        if (a % b == 0 || a == b)
        {
            cout << "0"
                 << "\n";
        }
        else{
            rem = a % b;
        cout << b - rem << endl;
        }
            
    }

    return 0;
}
